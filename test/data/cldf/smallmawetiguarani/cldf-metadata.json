        "row_cell_regexes": [
            "(?P<set>.*)",
            "(?P<Name>.*)",
            "(?P<English>.*)",
            "(?P<Spanish>.*)",
            "(?P<Portuguese>.*)",
            "(?P<French>.*)"
        ],
        "row_comment_regexes": [
            ".*",
            ".*",
            ".*",
            ".*",
            ".*",
            ".*"
        ],
        "lang_cell_regexes": [
            "(?P<Name>.*)",
            "(?P<Curator>.*)"
        ],
        "lang_comment_regexes": [
            ".*",
            ".*"
        ],
        "check_for_match": [
            "ID"
        ],
        "check_for_row_match": [
            "Name"
        ],
        "check_for_language_match": [
            "Name"
        ],
        "cell_parser": {
            "name": "MawetiCellParser",
            "form_separator": [
                ";",
                ","
            ],
            "variant_separator": [
                "~",
                "%"
            ],
            "add_default_source": "{1}",
            "cell_parser_semantics": [
                [
                    "(",
                    ")",
                    "comment",
                    false
                ],
                [
                    "{",
                    "}",
                    "source",
                    false
                ],
                [
                    "[",
                    "]",
                    "phonetic",
                    true
                ],
                [
                    "/",
                    "/",
                    "phonemic",
                    true
                ],
                [
                    "<",
                    ">",
                    "orthographic",
                    true
                ]
            ]
        },
        "cognates": {
            "row_cell_regexes": [
                "(?P<Set>.*)",
                "(?P<Name>.*)",
                ".*",
                ".*",
                ".*",
                ".*"
            ],
            "row_comment_regexes": [
                ".*",
                ".*",
                ".*",
                ".*",
                ".*",
                ".*"
            ],
            "lang_cell_regexes": [
                "(?P<Name>.*)",
                "(?P<Curator>.*)"
            ],
            "lang_comment_regexes": [
                ".*",
                ".*"
            ],
            "check_for_match": [
                "orthographic",
                "Language_ID",
                "phonemic",
                "phonetic",
                "source"
            ],
            "check_for_row_match": [
                "Name"
            ],
            "check_for_language_match": [
                "Name"
            ],
            "cell_parser": {
                "name": "MawetiCognateCellParser",
                "form_separator": [
                    ";",
                    ","
                ],
                "variant_separator": [
                    "~",
                    "%"
                ],
                "add_default_source": "{1}",
                "cell_parser_semantics": [
                    [
                        "(",
                        ")",
                        "comment",
                        false
                    ],
                    [
                        "{",
                        "}",
                        "source",
                        false
                    ],
                    [
                        "[",
                        "]",
                        "phonetic",
                        true
                    ],
                    [
                        "/",
                        "/",
                        "phonemic",
                        true
                    ],
                    [
                        "<",
                        ">",
                        "orthographic",
                        true
                    ]
                ]
            }
        }
    },
